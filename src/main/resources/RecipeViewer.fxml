<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" prefWidth="850.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@style.css">
        <VBox alignment="CENTER" spacing="10" HBox.hgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <HBox alignment="CENTER" maxHeight="10" prefHeight="5" VBox.vgrow="ALWAYS">
                <Label fx:id="nameLabel" HBox.hgrow="ALWAYS" />
            </HBox>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                <Label fx:id="ingredientLabel" HBox.hgrow="ALWAYS" />
            </HBox>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                <Label fx:id="instructionLabel" HBox.hgrow="ALWAYS" />
            </HBox>
        </VBox>
</HBox>
